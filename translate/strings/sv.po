#. Please leave 'Tekla' as it is
#: res/values/strings.xml:1
msgid "Tekla keyboard"
msgstr "Tekla tangentbord"

#. Please leave 'Tekla' as it is
#: res/values/strings.xml:2
msgid "Tekla keyboard settings"
msgstr "Tekla tangentbordsinställningar"

#: res/values/strings.xml:3
msgid "Input method"
msgstr "Inmatningsmetod"

#: res/values/strings.xml:4
msgid "Alternative input settings"
msgstr "Alternativa inmatningsinställningar"

#: res/values/strings.xml:5
msgid "Always show keyboard"
msgstr "Visa alltid tangentbord"

#: res/values/strings.xml:6
msgid "Shows a small navigation keyboard when not entering text"
msgstr "Visa navigationslist när ingen textinmatning görs"

#. Please leave 'Tekla Shield' as it is
#: res/values/strings.xml:7
msgid "Connect to Tekla Shield"
msgstr "Anslut till Tekla Shield"

#: res/values/strings.xml:8
msgid "Bluetooth is not supported"
msgstr "Inget bluetooth-stöd"

#: res/values/strings.xml:9
msgid "Bluetooth is disabled"
msgstr "Bluetooth är inte aktiverat"

#. Please leave 'Tekla Shield' as it is Also, the term 'check' here
#. is used in the context of a checkbox and thus it's
#. meaning is closer to 'select' than to 'review'.
#: res/values/strings.xml:10
msgid "Check to connect to a nearby Tekla Shield"
msgstr "Sök efter Tekla Shield"

#: res/values/strings.xml:11
msgid "Fullscreen switch mode"
msgstr "Helskärmstryckning för att välja"

#: res/values/strings.xml:12
msgid "Uses the entire screen as a single switch"
msgstr "Använder hela skärmen som en manöverkontakt"

#: res/values/strings.xml:13
msgid "Scanning settings"
msgstr "Inställningar för skanning"

#: res/values/strings.xml:14
msgid "Enable self-scanning"
msgstr "Aktivera automatisk skanning"

#: res/values/strings.xml:15
msgid "Scans the keyboard automatically"
msgstr "Skannar tangentbordet automatiskt"

#: res/values/strings.xml:16
msgid "Self-scanning delay"
msgstr "Tidsinställning för skanning"

#: res/values/strings.xml:17
msgid "Changes the self-scanning speed"
msgstr "Ändrar hastighet på automatisk skanning"

#: res/values/strings.xml:18
msgid "Enter new delay in milliseconds:"
msgstr "Ange ny hastighet i millisekunder:"

#. Please leave 'Tekla Shield' as it is
#: res/values/strings.xml:19
msgid "Switch event provider"
msgstr "Typ av externt styrsätt"

#: res/values/strings.xml:20
msgid "Tekla Shield connected"
msgstr "Tekla Shield är anslutet"

#: res/values/strings.xml:22
msgid "Tekla Shield disconnected"
msgstr "Tekla Shield frånkopplad"

#. Please leave 'Tekla Shield' as it is
#: res/values/strings.xml:22
msgid "Connecting to Tekla Shield"
msgstr "Ansluter till Tekla Shield"

#. Please leave 'Tekla Shield' as it is
#: res/values/strings.xml:23
msgid "Could not connect to Tekla Shield"
msgstr "Kunde inte ansluta till Tekla Shield"

#. Please leave 'Tekla Shields' as it is
#: res/values/strings.xml:24
msgid "No Tekla Shields in range"
msgstr "Inga Tekla Shields inom räckhåll"

#. Please leave 'Tekla Shield' as it is
#: res/values/strings.xml:25
msgid "Searching for Tekla Shields. Please wait..."
msgstr "Söker efter Tekla Shileds. Vänligen vänta..."

#: res/values/strings.xml:26
msgid "Fullscreen switch mode enabled"
msgstr "Helskärmskontakt aktiverad"

#: res/values/strings.xml:27
msgid "Fullscreen switch mode disabled"
msgstr "Helskärmskontakt inte aktiverad"

